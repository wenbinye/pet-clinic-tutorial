{
    "name": "winwin/pet-clinic",
    "type": "project",
    "require": {
        "php": "^7.0",
        "kuiper/boot": "^0.3",
        "kuiper/annotations": "^0.3",
        "twig/twig": "^2.3",
        "vlucas/phpdotenv": "^2.4",
        "monolog/monolog": "^1.22",
        "winwin/providers": "^0.2",
        "winwin/support": "^0.2",
        "vlucas/valitron": "^1.4",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "winwin/composer-phpcs-plugin": "^0.1",
        "filp/whoops": "^2.1",
        "phpunit/dbunit": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "winwin\\petClinic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "winwin\\petClinic\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "serve": "php -t public -S 0:${WEB_PORT:-8000} index.php"
    }
}
